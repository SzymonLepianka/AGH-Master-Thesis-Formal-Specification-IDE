<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<TitledPane fx:id="scenarioSelectorEditorRoot" animated="false" collapsible="false" prefHeight="475.0" prefWidth="360.0"
            text="Scenarios" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sl.fside.ui.editors.scenarioSelector.ScenarioSelectorEditorController">
    <AnchorPane fx:id="scenarioSelectorEditorAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="474.0"
                prefWidth="358.0">
        <ListView fx:id="scenarioList" id="scenarioListView" layoutX="9.0" layoutY="9.0" prefHeight="350.0"
                  prefWidth="338.0"/>
        <Button fx:id="addScenarioButton" alignment="CENTER" contentDisplay="CENTER"
                layoutX="9.0" layoutY="370.0" mnemonicParsing="false" onAction="#addScenarioButtonClicked"
                prefHeight="25.0"
                prefWidth="338.0" text="Add an additional scenario"/>
    </AnchorPane>
</TitledPane>

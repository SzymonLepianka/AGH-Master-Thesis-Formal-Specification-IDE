<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="scenarioRoot" prefHeight="50.0" prefWidth="290.0"
            xmlns="http://javafx.com/javafx/17.0.2-ea"
            fx:controller="sl.fside.ui.editors.scenarioSelector.controls.ScenarioController">
    <TextField fx:id="scenarioNameTextField" layoutX="7.0" layoutY="14.0" prefHeight="25.0" prefWidth="220.0"
               onKeyTyped="#scenarioNameChanged"/>
    <CheckBox fx:id="isMainScenarioCheckBox" layoutX="235.0" layoutY="4.0" mnemonicParsing="false" text="isMain?"/>
    <Button fx:id="removeScenarioButton" layoutX="235.0" layoutY="25.0" mnemonicParsing="false" prefHeight="15.0"
            prefWidth="62.0" text="Remove" onAction="#onRemoveScenarioButtonClicked"/>
</AnchorPane>

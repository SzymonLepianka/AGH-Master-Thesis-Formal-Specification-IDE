<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<TitledPane fx:id="useCaseSelectorEditorRoot" animated="false" collapsible="false" prefHeight="475.0" prefWidth="360.0"
            text="Use cases" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sl.fside.ui.editors.useCaseSelector.UseCaseSelectorEditorController">
    <AnchorPane fx:id="useCaseSelectorEditorAnchorPane" minHeight="0.0" minWidth="0.0" prefHeight="474.0"
                prefWidth="358.0">
        <ListView fx:id="useCasesList" layoutX="9.0" layoutY="9.0" prefHeight="350.0" prefWidth="338.0"/>
        <Button fx:id="addOptionalUseCaseButton" alignment="CENTER" contentDisplay="CENTER"
                layoutX="9.0" layoutY="370.0" mnemonicParsing="false" onAction="#addUseCaseButtonClicked" prefHeight="25.0"
                prefWidth="338.0" text="Add an optional Use Case"/>
        <Label fx:id="currentlySelectedUseCaseLabel" alignment="CENTER" layoutX="9.0" layoutY="410.0" prefHeight="25.0"
               prefWidth="338.0" text="placeholder_text"/>
    </AnchorPane>
</TitledPane>

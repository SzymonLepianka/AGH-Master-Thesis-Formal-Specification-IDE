<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane fx:id="actionRoot" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sl.fside.ui.editors.actionEditor.controls.ActionController"
            prefHeight="90.0" prefWidth="318.0">
    <ScrollPane fx:id="actionScrollPans" prefHeight="45.0" prefWidth="302.0" layoutX="7.0" layoutY="5.0">
        <TextFlow fx:id="textFlow" layoutX="50.0" layoutY="7.0" prefHeight="36.0" prefWidth="250.0"/>
    </ScrollPane>
    <Button fx:id="removeButton" layoutX="10.0" layoutY="55.0" mnemonicParsing="false" prefHeight="15.0"
            prefWidth="110.0"
            text="Remove action" onAction="#onRemoveActionButtonClicked"/>
</AnchorPane>

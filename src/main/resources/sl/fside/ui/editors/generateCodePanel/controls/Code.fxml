<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="codeRoot" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sl.fside.ui.editors.generateCodePanel.controls.CodeController"
            prefHeight="100.0" prefWidth="318.0">
    <TextArea fx:id="textArea" layoutX="7.0" layoutY="7.0" prefHeight="60.0" prefWidth="308.0" wrapText="true"
              promptText="Instruction (source code)" onKeyTyped="#codeContentChanged"/>
    <Button fx:id="removeButton" layoutX="7.0" layoutY="70.0" mnemonicParsing="false" prefHeight="15.0"
            prefWidth="60.0" text="Remove" onAction="#onRemoveButtonClicked"/>
    <ComboBox fx:id="languageComboBox" layoutX="70.0" layoutY="70.0" prefHeight="15.0" prefWidth="95.0"
              promptText="Language" onAction="#onLanguageComboBoxClicked"/>
    <ComboBox fx:id="atomicActivityComboBox" layoutX="170.0" layoutY="70.0" prefHeight="15.0" prefWidth="145.0"
              promptText="Atomic activity" onAction="#onAtomicActivityComboBoxClicked"/>
</AnchorPane>
